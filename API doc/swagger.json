{
    "swagger": "2.0",
    "info": {
        "title": "Project Transparency",
        "description": "The API doc for Project Transparency which is Burberry's single source of truth for all digital merchandising",
        "version": "1.0.0"
    },
    "host": "api.test.project-transparency.openly.co",
    "schemes": [
        "https"
    ],
    "basePath": "/",
    "produces": [
        "application/json"
    ],
    "paths": {
        "/version": {
            "get": {
                "summary": "Get version of the application",
                "description": "The response will have the current version of the application\n",
                "tags": [
                    "Version"
                ],
                "responses": {
                    "200": {
                        "description": "Version of the application",
                        "schema": {
                            "type": "string",
                            "description": "The version of the app"
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "Error": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "format": "int32"
                },
                "message": {
                    "type": "string"
                }
            }
        }
    }
}